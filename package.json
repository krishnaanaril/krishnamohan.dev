{
  "name": "angular-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5000",
    "build": "ng build",
    "build-prod": "cross-env NODE_ENV=production ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-static": "npm run scully -- --nw",
    "replace-files": "rm -r ./dist/angular-portfolio/ && cp -r ./dist/static/ ./dist/angular-portfolio",
    "gh-deploy": "ng deploy --no-silent --no-build --cname=krishnamohan.dev",
    "pre-deploy": "npm run build-prod && npm run build-static && npm run replace-files",
    "deploy": "npm run pre-deploy && npm run gh-deploy",
    "scan-routes": "npm run scully -- --scanRoutes",
    "serve-static": "npm run scully serve",
    "serve-local": "npm run build-prod && npm run build-static && npm run serve-static",
    "scully": "scully",
    "scully:serve": "scully serve",
    "scully:new-post": "ng generate @scullyio/init:post",
    "prettier": "prettier",
    "format:test": "prettier --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:write": "prettier --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^14.0.3",
    "@angular/animations": "^14.0.3",
    "@angular/cdk": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/localize": "^14.0.3",
    "@angular/material": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "@angular/service-worker": "^14.0.3",
    "@scullyio/init": "^1.1.1",
    "@scullyio/ng-lib": "^1.1.0",
    "@scullyio/scully": "^1.1.0",
    "core-js": "^2.5.4",
    "prismjs": "^1.23.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@angular/language-service": "^14.0.3",
    "@gammastream/scully-plugin-sitemap": "^1.0.7",
    "@notiz/scully-plugin-rss": "^1.1.0",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jasmine": "^3.6.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.4",
    "autoprefixer": "^10.2.4",
    "codelyzer": "^6.0.2",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^7.0.3",
    "fast-xml-parser": "^3.18.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webp": "^5.1.0",
    "ini": "^2.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^11.0.0",
    "ngx-tailwind": "^2.1.0",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.0.4",
    "protractor": "^7.0.0",
    "scully-plugin-webpimages": "0.0.5",
    "tailwindcss": "^2.0.3",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "^4.7.4",
    "xmlbuilder": "^15.1.1"
  }
}
